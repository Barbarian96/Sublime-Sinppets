<snippet>
    <content><![CDATA[
    
    vector<int>primefactor;
  while (n % 2 == 0)  
    {  
        primefactor.pb(2);
        n = n/2;  
    }  
    for(int i = 3; i <= sqrt(n); i = i + 2)  
    {  
        while (n % i == 0)  
        {  
            primefactor.pb(i);
            n = n/i;  
        }  
    }  
    if (n > 2)  primefactor.pb(n);


]]></content>

    
    <tabTrigger>primefactor</tabTrigger> 
    
    
</snippet>